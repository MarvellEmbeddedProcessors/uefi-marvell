                        i2c@701000 {
                                compatible = "marvell,mv78230-i2c";
                                reg = <0x701000 0x20>;
                                #address-cells = <0x1>;
                                #size-cells = <0x0>;
                                interrupts = <0x0 0x78 0x4>;
                                clock-names = "core", "reg";
                                clocks = <0x8 0x1 0x15 0x8 0x1 0x11>;
                                status = "okay";
