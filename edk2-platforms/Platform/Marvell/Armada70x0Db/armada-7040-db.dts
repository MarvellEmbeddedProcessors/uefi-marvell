                                        pinctrl-names = "default";
                                        gpio-controller;
                                        #gpio-cells = <0x2>;
                                        reg = <0x21>;
                                        phandle = <0x14>;
                                };
                        };

                        i2c@701100 {
                                compatible = "marvell,mv78230-i2c";
                                reg = <0x701100 0x20>;
                                #address-cells = <0x1>;
                                #size-cells = <0x0>;
                                interrupts = <0x0 0x79 0x4>;
                                clock-names = "core", "reg";
                                clocks = <0x8 0x1 0x15 0x8 0x1 0x11>;
                                status = "disabled";
