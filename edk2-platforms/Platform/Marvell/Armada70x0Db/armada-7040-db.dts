                        spi@700680 {
                                compatible = "marvell,armada-380-spi";
                                reg = <0x700680 0x50>;
                                #address-cells = <0x1>;
                                #size-cells = <0x0>;
                                clock-names = "core", "axi";
                                clocks = <0x8 0x1 0x15 0x8 0x1 0x11>;
                                status = "disabled";

                                spi-flash@0 {
                                        #address-cells = <0x1>;
