                                ethernet-phy@0 {
                                        reg = <0x0>;
                                        phandle = <0xb>;
                                };

                                ethernet-phy@1 {
                                        reg = <0x1>;
                                        phandle = <0xd>;
                                };
                        };

                        mdio@12a600 {
                                #address-cells = <0x1>;
                                #size-cells = <0x0>;
                                compatible = "marvell,xmdio";
                                reg = <0x12a600 0x10>;
                                clocks = <0x8 0x1 0x5 0x8 0x1 0x6 0x8 0x1 0x12>;
                                status = "disabled";
                        };

                        interrupt-controller@1e0000 {
                                compatible = "marvell,cp110-icu";
                                reg = <0x1e0000 0x440>;
                                #interrupt-cells = <0x3>;
                                interrupt-controller;
                                msi-parent = <0xe>;
                                phandle = <0x7>;
