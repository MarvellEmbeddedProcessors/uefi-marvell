                        mdio@12a200 {
                                #address-cells = <0x1>;
                                #size-cells = <0x0>;
                                compatible = "marvell,orion-mdio";
                                reg = <0x12a200 0x10>;
                                clocks = <0x8 0x1 0x9 0x8 0x1 0x5 0x8 0x1 0x6 0x8 0x1 0x12>;
