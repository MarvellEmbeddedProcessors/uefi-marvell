                                dma-coherent;
                                interrupts = <0x0 0x6a 0x4>;
                                clock-names = "core", "reg";
                                clocks = <0x8 0x1 0x16 0x8 0x1 0x10>;
                                status = "okay";
                                usb-phy = <0x10>;
