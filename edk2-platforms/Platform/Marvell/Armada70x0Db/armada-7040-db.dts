                        usb3@510000 {
                                compatible = "marvell,armada-8k-xhci", "generic-xhci";
                                reg = <0x510000 0x4000>;
                                dma-coherent;
                                interrupts = <0x0 0x69 0x4>;
                                clock-names = "core", "reg";
                                clocks = <0x8 0x1 0x17 0x8 0x1 0x10>;
                                status = "okay";
                                usb-phy = <0x11>;
