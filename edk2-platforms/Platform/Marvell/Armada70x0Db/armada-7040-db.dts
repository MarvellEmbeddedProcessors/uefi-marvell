                        serial@702300 {
                                compatible = "snps,dw-apb-uart";
                                reg = <0x702300 0x100>;
                                reg-shift = <0x2>;
                                interrupts = <0x0 0x7d 0x4>;
                                reg-io-width = <0x1>;
                                clock-names = "baudclk", "apb_pclk";
                                clocks = <0x8 0x1 0x15 0x8 0x1 0x11>;
                                status = "disabled";
