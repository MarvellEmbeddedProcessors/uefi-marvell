                        sdhci@6e0000 {
                                compatible = "marvell,armada-ap806-sdhci";
                                reg = <0x6e0000 0x300>;
                                interrupts = <0x0 0x10 0x4>;
                                clock-names = "core";
                                clocks = <0x4 0x4>;
